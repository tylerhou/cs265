@main {
    one: int = const 1;
    four: int = const 4;
    a: ptr<int> = alloc four;
    b: ptr<int> = alloc four;
    c: ptr<int> = ptradd a one;

    ten: int = const 10;
    twenty: int = const 20;

    cond: bool = const true;
    br cond .t .f;
.t:
    store a ten;
    store b ten;
    jmp .end;
.f:
    store a ten;
    store b twenty;
    jmp .end;
.end:
    al: int = load a;
    bl: int = load b;
    print a;
    print b;
}
